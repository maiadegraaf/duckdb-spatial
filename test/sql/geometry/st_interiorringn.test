require spatial

foreach TYPE GEOMETRY POLYGON_2D

query I
select st_interiorringn(
    st_geomfromtext('POLYGON((0 0,10 0,10 10,0 10,0 0),
                             (2 2,4 2,4 4,2 4,2 2),
                             (6 6,8 6,8 8,6 8,6 6))')::${TYPE},
    2
);
